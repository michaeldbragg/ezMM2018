                            "{TemplateBinding SnapsToDevicePixels}" Margin="2,2,2,2"
                            x:Name="PART_SelectedContentHost"
                            ContentSource="SelectedContent"/>
                                                        </Microsoft_Windows_Themes:ClassicBorderDecorator>
                                                    </Grid>
