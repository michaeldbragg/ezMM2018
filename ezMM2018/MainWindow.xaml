                                <GradientStop Color="#FFBABABA" Offset="1" />
                                <GradientStop Color="White" Offset="0" />
                                <GradientStop Color="#FFE6E6E6" Offset="0.164" />
                            </LinearGradientBrush>
                        </Grid.Background>
                    </Grid>

                </TabItem>
                <!--Reports Tab Item-->
                <TabItem x:Name="tiReports" Header="Reports" Height="45" Background="Firebrick" Width="180" Margin="-2,0,0,0" VerticalAlignment="Bottom">

                </TabItem>
               
            </TabControl>
            <StatusBar Name="mainStatus" Height="45" DockPanel.Dock="Bottom">

                <TextBox x:Name="txtSearch" Width="125" Height="25" Margin="5"/>
                <Image Source="/images/search1.ico" Width="30" Margin="0,0,200,0"/>

                <Button Name="btnFirst" Content="First" Margin="25, 0,0,0" Height="35" Width="50" Background="Yellow"  />
                <Button Name="btnPrevious" Content="Previous" Margin="10,0,0,0" Height="35" Width="50" />

                <Button Name="btnNext" Content="Next" Margin="20,0,0,0" Height="35" Width="50" />
                <Button Name="btnLast" Content="Last" Margin="10,0,0,0" Height="35" Width="50" Background="Blue" />

                <Button Name="btnSave" Content="Save" Margin="75,0,0,0" Width="50" Height="35" Background="Green" />
                <Button Name="btnDelete" Content="Delete" Margin="10,0,0,0" Height="35" Width="50" Background="Red" />
                <Button Name="btnClear" Content="Clear" Margin="10,0,0,0" Height="35" Width="50" Background="white" />
            </StatusBar>

        </DockPanel>
    </ScrollViewer>
</Window>
