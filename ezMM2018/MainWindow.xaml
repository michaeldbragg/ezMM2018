                        <DataTemplate>
                            <StackPanel Background="White">
                                <StackPanel Orientation="Horizontal">
                                    <Border BorderBrush="DarkGray" BorderThickness="2" CornerRadius="5">
                                        <Image Source="/images/ImagePlaceHolder.png" x:Name="imgDeceased" Width="50" Height="50"></Image>
                                    </Border>
                                    <StackPanel >
                                        <TextBlock Text="{Binding Last}" Name="lstBoxLast" FontSize="10" Height="12" Margin="2"/>
                                        <TextBlock Text="{Binding First}" Name="lstBoxFirst" FontSize="10" Height="12" Margin="2"/>
                                        <TextBlock Text="{Binding Middle}" Name="lstBoxMiddle" FontSize="10" Height="12" Margin="2" />
                                    </StackPanel>
                                </StackPanel>
                                <TextBlock Text="{Binding DoD}" Name="lstBoxDoD" FontSize="10" Height="12" Margin="2" HorizontalAlignment="Center" />
                            </StackPanel>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                </ListBox>
            </Expander>
